    "@lezer/highlight": "^1.2.0",
    "crelt": "^1.0.6"
  },
  "devDependencies": {
    "@codemirror/lang-xml": "^6.1.0",
    "@marijn/buildtool": "^1.0.0",
    "codemirror": "^6.0.1",
